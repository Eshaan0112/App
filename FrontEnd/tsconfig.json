{
    "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "jsx": "react",
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "exclude": ["node_modules"]
  }
  